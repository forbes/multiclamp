{
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "rules": {
        "indent": ["error", "tab"],
        "max-len": ["error", 140],
        "no-tabs": "off"
    },
    "globals": {
        "jasmine": false,
        "spyOn": false,
        "test": false,
        "expect": false,
        "beforeEach": false,
        "describe": false
    }
}